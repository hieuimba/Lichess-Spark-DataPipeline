{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-guess-the-elo-dev"
		},
		"ls_cosmosdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_cosmosdb'"
		},
		"ls_datalakestorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalakestorage'"
		},
		"ls_datalakestorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://guesstheelostoragedev.dfs.core.windows.net/"
		},
		"ls_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-guess-the-elo-dev.vault.azure.net/"
		},
		"ls_lichess_database_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://database.lichess.org/standard/"
		},
		"tr_main_weekly_properties_pl_main_parameters_Month": {
			"type": "string",
			"defaultValue": "@{formatDateTime(trigger().startTime,'yyyy-MM')}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-guess-the-elo-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_cosmosdb_connectionString": {
						"value": "[parameters('ls_cosmosdb_connectionString')]"
					},
					"ls_datalakestorage_accountKey": {
						"value": "[parameters('ls_datalakestorage_accountKey')]"
					},
					"ls_datalakestorage_properties_typeProperties_url": {
						"value": "[parameters('ls_datalakestorage_properties_typeProperties_url')]"
					},
					"ls_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_properties_typeProperties_baseUrl')]"
					},
					"ls_lichess_database_properties_typeProperties_url": {
						"value": "[parameters('ls_lichess_database_properties_typeProperties_url')]"
					},
					"tr_main_weekly_properties_pl_main_parameters_Month": {
						"value": "[parameters('tr_main_weekly_properties_pl_main_parameters_Month')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}